            <ComboBoxItem Content="Jay" HorizontalAlignment="Left" Width="223" Selected="ComboBoxItem_Selected"/>
            <ComboBoxItem Content="Seth" HorizontalAlignment="Left" Width="223" Selected="ComboBoxItem_Selected_1"/>
            <ComboBoxItem Content="Jerry" HorizontalAlignment="Left" Width="223"/>
            <Border HorizontalAlignment="Center" Width="223"/>
            <Border HorizontalAlignment="Left" Width="223"/>
        </ComboBox>-->
